# Managed by Puppet, DO NOT TOUCH!

network:
  host: _tap0:ipv4_

transport:
  tcp:
    port: <%= esTCPPortRange %>

http:
  port: <%= esHTTPPortRange %>

cluster:
  name: "<%= cluster %>"

path:
  logs: <%= esLogPath %>
  data: <%= esDataPath %>

boostrap:
  mlockall: true

discovery.zen.ping.multicast.enabled: true

